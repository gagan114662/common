# Environment Variables Template
# Copy this file to .env and fill in your actual values

# QuantConnect API Credentials
QUANTCONNECT_TOKEN=your_quantconnect_token_here
QUANTCONNECT_USER_ID=357130

# OpenAI API (for AI collaboration features)
OPENAI_API_KEY=your_openai_api_key_here

# Multi-AI Collaboration Server
GEMINI_API_KEY=your_gemini_api_key_here
GROK_API_KEY=your_grok_api_key_here
DEEPSEEK_API_KEY=your_deepseek_api_key_here

# Database Configuration
DATABASE_URL=sqlite:///data/knowledge_base.db

# System Configuration
DEPLOYMENT_ENV=development
LOG_LEVEL=INFO
GPU_ENABLED=true

# Security Configuration
MASTER_ENCRYPTION_KEY=your_master_key_here
HSM_ENABLED=false

# Monitoring and Alerting
DASHBOARD_PORT=8080
PROMETHEUS_ENABLED=false
GRAFANA_ENABLED=false

# Performance Tuning
MAX_CONCURRENT_BACKTESTS=50
MEMORY_LIMIT_GB=64
GPU_MEMORY_FRACTION=0.85